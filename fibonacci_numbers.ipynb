   ],
   "source": [
    "limit = int(input(\"Enter the limit of fibonacci numbers to generate :\"))\n",
    "a = 1\n",
    "b = 1\n",
    "total = 0\n",
    "fibo_list = [a]\n",
    "while b < limit:\n",
    "    fibo_list.append(b)\n",
    "    total = a + b\n",
    "    a = b\n",
    "    b = total\n",
    "      \n",
    "print(\"From 1 to {} fibonacci numbers are :\".format(limit), (fibo_list))\n"
   ]
